{
    "manifest_version": 2,
    "name": "PhotoCopy",
    "description": "Copies the code (or text) inside a screenshot",
    "version": "0.1",
    "browser_action": {
        "default_title": "hello"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "activeTab"
    ]
}